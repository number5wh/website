<script type="text/javascript" language="javascript">
{literal}
var roomRobot={
	/*点击显示修改服务器配置界面*/
	BindEvent:function(){
		/*点击显示修改服务器配置界面*/
		$('.edit').each(function(){
			$(this).unbind();
			$(this).click(function(){
{/literal}
				setting.Url='{au d=YunWei c=RoomRobot a=showAddRoomRobotHtml}';
{literal}
				ajax.RequestUrl(setting.Url,'RoomID='+$(this).attr('id'),'callback.ShowAddRoomRobotHtml');
			});	
		});

		/*点击删除机器人配置信息*/
			$('.del').each(function(){
				$(this).click(function(){
		{/literal}
					setting.Url='{au d=YunWei c=RoomRobot a=delRoomRobot}';
		{literal}
					if(confirm('删除后将无法恢复,确定删除?'))
						ajax.RequestUrl(setting.Url,'RoomID='+$(this).attr('id'),'callback.DelRoomRobot');
				});	
			});
            $('.active').each(function(){
                $(this).click(function(){
                    {/literal}
                    setting.Url = '{au d=YunWei c=RoomRobotBehavior a=activeRoomRobot}';
                    {literal}
                    ajax.RequestUrl(setting.Url,'RoomID='+$(this).attr('id'),'callback.ActiveRoomRobot')
                })
            })
    }
  }


			
{/literal}
</script>

<table width="100%" border="0" cellpadding="1" cellspacing="1" class="tbList">
  <tr>
    <th width="10%" bgcolor="#FFFFFF">房间ID</th>
    <th width="15%" bgcolor="#FFFFFF">游戏房间名称</th>
    <th width="15%" bgcolor="#FFFFFF">最大机器人数</th>
    <th width="15%" bgcolor="#FFFFFF">胜利权值</th>
    <th width="15%" bgcolor="#FFFFFF">需要赢的钱数</th>
    <th width="15%" bgcolor="#FFFFFF">服务的桌子数</th>
    <th width="15%" bgcolor="#FFFFFF">操作</th> 
  </tr>
  {foreach from=$RobotList item=robot}
  <tr id="Row_{$robot.UserID}">
  	<td align ="center" bgcolor='#FFFFFF'>{$robot.RoomID}</td>
    <td align="center" bgcolor="#FFFFFF">{$robot.RoomName}</td>
    <td align="center" bgcolor="#FFFFFF">{$robot.MaxCount}</td>
    <td align="center" bgcolor="#FFFFFF">{$robot.RobotWinWeighted}</td>
    <td align="center" bgcolor="#FFFFFF">{$robot.RobotWinMoney}</td>
    <td align="center" bgcolor="#FFFFFF">{$robot.ServiceTables}</td>
    <td align="center" bgcolor="#FFFFFF">
   	    <a href="javascript:void(0)" class="active" id="{$robot.RoomID}">激活</a>
   	    <a href="javascript:void(0)" class="edit" id="{$robot.RoomID}">修改</a>
   	    <a href="javascript:void(0)" class="del" id="{$robot.RoomID}">删除</a>
	</td>
  </tr> 
  {foreachelse}
  <tr>
    <td colspan="6" align="center" bgcolor="#FFFFFF">暂无记录</td>
  </tr>
  {/foreach}
  {if $RobotList}
  <tr>
    <td colspan="9" align="center" bgcolor="#FFFFFF">{include file="$skin/Inc/Page.inc.html"}</td>
  </tr>
  {/if}
</table>