        <th width="20%" bgcolor="#FFFFFF">玩家账号</th>
        <th width="20%" bgcolor="#FFFFFF">商人总转入</th>
        <th width="20%" bgcolor="#FFFFFF">在线总充值</th>
        <th width="20%" bgcolor="#FFFFFF">总转出</th>
    </tr>
    {foreach from=$UserDataCountList item=List}
