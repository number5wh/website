<!--
{$Refresh}:关闭弹出层的时候,是否刷新,true:刷新,false:不刷新
-->
<div class="sysWin">
	<h2 class="f2 white">
		<div class="tLeft f1 left"></div>提示信息	
		<div class="tRight f1 right"></div>
		<a title="关闭" class="btnCls f3 right" href="javascript:void(0);" onclick="main.CloseMsgBox({$Refresh},'{$ClassName}')"/></a>
		<br class="clear" />
	</h2>
	
	<div class="bdy">
    	<div style="width:100%;height:100px;background-color:#ffffff;text-align:center;line-height:100px">
		{$resultMsg}
        </div>
	</div>

	<div class="bot f2">
		<div class="botLeft f1 left"></div>
		<div class="botRight f1 right"></div>
		<input type="button" class="btn f2 right" value="{$btnValue}" onclick="{$click}"/>
        <div class="orange right" id="AddServerMsg"></div>
		<br class="clear" />
	</div>
</div>