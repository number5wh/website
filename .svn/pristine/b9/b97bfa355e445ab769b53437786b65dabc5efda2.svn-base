<script type="text/javascript" language="javascript">
    {literal}
    $('#btnRobotCreator').click(function(){
        {/literal}
        setting.Url='{au d=YunWei c=RobotCreator a=addMulRobot}';
        {literal}
        var NumM = $('#NumM').val();
        var NumN = $('#NumN').val();
        var RoomID = $('#RoomID').val();
        var ServiceTime = 0;
        $('.ServiceTime').each(function(){
            if($(this).attr('checked')) ServiceTime += parseInt($(this).val());
        });
        var MinTakeScore = $('#MinTakeScore').val();
        var MaxTakeScore = $('#MaxTakeScore').val();
        var MinPlayDraw = $('#MinPlayDraw').val();
        var MaxPlayDraw = $('#MaxPlayDraw').val();
        var MinReposeTime = $('#MinReposeTime').val();
        var MaxReposeTime = $('#MaxReposeTime').val();
        var ServiceGender = 0;

        $('.ServiceGender').each(function(){
            if($(this).attr('checked')) ServiceGender += parseInt($(this).val());
        });
        if(isNaN(NumM) || isNaN(NumN)){
            $('#AddRobotUserMsg').html('M必须小于N');
            return false;
        }
        setting.Params='NumN='+NumN+'&NumM='+NumM+'&RoomID='+RoomID+'&ServiceTime='+ServiceTime+'&MinTakeScore='+MinTakeScore+'&MaxTakeScore='+MaxTakeScore+'&MinPlayDraw='+MinPlayDraw+'&MaxPlayDraw='+MaxPlayDraw+'&MinReposeTime='+MinReposeTime+'&MaxReposeTime='+MaxReposeTime+'&ServiceGender='+ServiceGender;
        ajax.RequestUrl(setting.Url,setting.Params,'callback.AddMulRobot');
    });

    $('#btnCancel').click(function(){
        main.CloseMsgBox(false,'');
    })
    {/literal}
</script>