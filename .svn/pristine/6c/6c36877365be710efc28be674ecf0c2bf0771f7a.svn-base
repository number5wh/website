@charset "utf-8";

/* 轮播 */
.slideBox{position:relative;left:50%;margin-left:-960px;height:420px;}
.slideBox .hd{position:absolute;z-index:557;left:900px;top:378px;}
.slideBox .hd li{float:left;background:#1c5896;border-radius:50%;width:10px;height:10px;margin:0 6px;}
.slideBox .hd .on{background:#fff;}
.slideBox .w1100{position:relative;margin:0 auto;width:1100px;z-index:556}
/* 下载部分 */
.download{width:280px;height:320px;position:absolute;top:40px;left:1050px;text-align:center;color:#fff;}
.download h3{font-size:44px;}
.download h4{font-size:26px;}
.download-icon>.icon{width:80px;height:100px;margin:0 6px 10px;padding-top:10px;cursor:pointer;}
.download-icon>.icon a{color:#fff;}
.btn-dow{width:280px;height:56px;border-radius:6px;background:rgba(0,0,0,0.4);display:block;font-size:26px;line-height:56px;color:#fff;}
.btn-dow:hover{color:#fff;background:rgba(0,0,0,0.46);}
.code-dow:hover .qr-code{display:block;}
.qr-code{position: absolute;display:none;top: -20px;left: 150px;width: 160px;padding: 20px 0;text-align: center;background: #fff; cursor: initial;
    box-shadow: 0 -1px 2px 0 rgba(0,0,0,.21);font-size: 14px;color:#333;}
.qr-code p{padding-top:10px;margin:0;}

/* 主体部分 */
.article{padding:20px 10px;background:#fff;margin:20px 0 10px;}
.new-activity{border:1px solid #239fe7;width:660px;height:46px;margin-top:4px;}
.new-activity .fl{background:#239fe7;color:#fff;text-align:center;font-size:18px;width:134px;height:44px;line-height:44px;}
.new-activity .fr{font-size:12px;width:510px;height:32px;overflow:hidden;line-height:34px;margin:6px 0;}
.infoList li{ white-space: nowrap; overflow: hidden; text-overflow: ellipsis}

.game-title{margin:40px 0 10px;color:#333;font-size:20px;}
.game-picScroll{width:660px;}
.game-picScroll .bd{width:600px;height:134px;margin:0 30px;padding-top:10px;}
/*.game-picScroll .gameList{width:580px;height:124px;padding:0 30px;}*/
.game-picScroll .hd{position:relative;}
.game-picScroll .hd .glyphicon{font-size:24px;color:#239fe7;}
.game-picScroll .hd .glyphicon:hover{text-decoration:none;}
.game-picScroll .hd .next{position:absolute;top:52px;left:2px;}
.game-picScroll .hd .prev{position:absolute;top:52px;right:2px;}
.game-picScroll .gameList li{width:100px;height:134px;margin:0 10px;padding-top:10px;transition-duration:0.2s;-webkit-transition-duration:0.2s;}
.game-picScroll .gameList li:hover{margin-top:-6px;}
.game-picScroll .gameList li:hover a{color:#0a82c8;}
.game-picScroll .gameList img{border-radius:12px;width:96px;height:96px;}
.gameList .title{text-align:center;margin-top:4px;}
.gameList .title a{font-size:16px;color:#666;}
.box-btn{height:60px;margin:4px 3px;line-height:60px;background:#229fe7;display:inline-block;text-align:center;color:#fff;border-radius:6px;}
.btn1{width:146px;}
.btn2{width:300px;}
.btn3{background:#15b59a;}
.box-btn:hover {text-decoration:none;color:#fff;font-size:14.6px;
	background: -moz-linear-gradient(top, #229fe7 0%,#0a82c8 50%, #229fe7 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#229fe7),color-stop(50%,#0a82c8), color-stop(100%,#229fe7));
    background: -webkit-linear-gradient(top, #229fe7 0%,#0a82c8 50%,#229fe7 100%);
    background: -o-linear-gradient(top, #229fe7 0%,#0a82c8 50%,#229fe7 100%);
    background: -ms-linear-gradient(top, #229fe7 0%,#0a82c8 50%,#229fe7 100%);
	background:linear-gradient(to bottom, #229fe7 0%,#0a82c8 50%,#229fe7 100%);}
.btn3:hover{
	background: -moz-linear-gradient(top, #15b59a 0%,#2d9191 50%, #15b59a 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#15b59a),color-stop(50%,#2d9191), color-stop(100%,#15b59a));
    background: -webkit-linear-gradient(top, #15b59a 0%,#2d9191 50%,#15b59a 100%);
    background: -o-linear-gradient(top, #15b59a 0%,#2d9191 50%,#15b59a 100%);
    background: -ms-linear-gradient(top, #15b59a 0%,#2d9191 50%,#15b59a 100%);
	background:linear-gradient(to bottom, #15b59a 0%,#2d9191 50%,#15b59a 100%);}



.lists .title{border-left:5px solid #229fe7;font-size:20px;color:#333;}
.lists .title a{font-size:12px;color:#999;}
.box-lists{margin-top:26px;}
.box-lists .list{height:42px;line-height:42px;border-bottom:1px solid #eeeeee;font-size:16px;}
.box-lists .list a{color:#666;}
.box-lists .list a:hover{color:#229fe7;}
.box-lists .list a span{display:inline-block;width:360px;height:42px;overflow: hidden; text-overflow:ellipsis; white-space: nowrap;}
.box-lists .list i{float:right;color:#999;}

